{{#if option.showVideo}}   <video        class='player-video'        webkit-playsinline        playsinline        {{#if option.src}}            src={{option.src}}        {{/if}}        {{#if option.autoplay}}                  autoplay        {{/if}}        {{#if option.muted}}                         muted        {{/if}}        {{#if option.preload}}                    preload={{option.preload}}         {{/if}}   ></video>{{/if}}