<div class='player-mask js-click-mask' ></div><div class='player-context-mask js-context-mask' ></div><div class='player-container'>{{#if showVideo}}   <video        class='player-video'        webkit-playsinline        playsinline        {{#if option.src}}            src={{option.src}}        {{/if}}        {{#if option.autoplay}}                  autoplay        {{/if}}        {{#if option.muted}}                         muted        {{/if}}        {{#if option.preload}}                    preload={{option.preload}}         {{/if}}   ></video> {{/if}} {{#if option.video.pic}}   <div class='player-poster' style="background-image: url({{option.video.pic}});"></div> {{/if}}</div><div class=player-controller><div class='player-time'>    <div class='player-time-bar js-time-bar'>       <div class='player-time-bar-load js-load-bar'></div>        <div class='player-time-bar-line js-timeBar-line'>            <span class='player-time-bar-thumb js-timeThumb-btn'></span>        </div>    </div></div><div class=player-controller-left>  <button class='player-btn js-play-btn' data-tip='show' data-tip-txt='播放 k'>   {{{ Icon.Play}}}  </button>  <div class="player-volume">    <button class="player-volume-btn js-volume-btn" data-tip='show' data-tip-txt='音量 m'>         {{{ Icon.Volume}}}    </button>    <div class="player-volume-bar js-volume-bar">        <div class='player-volume-bar-line js-volume-bar-line'>            <div class='player-volume-bar-thumb js-thumb-btn'></div>        </div>    </div>  </div>  <div class='player-time-duration'>  <span class='player-starttime'>0:00</span>  /  <span class='player-endtime'>0:00</span>  </div></div><div class=player-controller-right>   <div class='player-setting'>        <button class='player-setting-btn js-setting-btn' data-tip='show' data-tip-txt='设置'>                     {{{ Icon.Seting}}}        </button>        <div class='player-setting-container js-setting-panel'>        </div>   </div>   <div class='player-full'>        <button class='player-full-btn js-full-btn' data-tip='show' data-tip-txt='全屏 f'>                   {{{ Icon.FullScreen}}}        </button>   </div></div></div><div class='player-context-panel js-context-panel'></div><div class='player-notice js-notice'></div>